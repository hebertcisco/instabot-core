{
  "name": "instabot-core",
  "version": "1.0.1",
  "description": "Instagram comment generator robot",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hebertcisco/instabot-core.git"
  },
  "keywords": [
    "boilerplate",
    "typescript"
  ],
  "author": "Hebert Cisco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hebertcisco/instabot-core/issues"
  },
  "homepage": "https://github.com/hebertcisco/instabot-core#readme",
  "dependencies": {
    "axios": "^0.24.0",
    "beautlog": "^0.3.0",
    "chrome-launcher": "^0.14.0",
    "chromium": "^3.0.2",
    "dotenv": "^10.0.0",
    "pensador-api": "^1.0.0",
    "puppeteer": "^13.0.0",
    "puppeteer-core": "^13.0.0"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/jest": "27.0.3",
    "@types/puppeteer": "5.4.4",
    "@typescript-eslint/eslint-plugin": "5.8.1",
    "@typescript-eslint/parser": "5.8.1",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "eslint-plugin-jest": "25.7.0",
    "jest": "27.4.5",
    "nodemon": "2.0.15",
    "ts-jest": "27.1.2",
    "typescript": "4.0.3"
  },
  "files": [
    "lib/**/*"
  ]
}
